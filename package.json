{
  "name": "cognitive-agent-wrapper",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --runInBand --testPathPattern=tests/e2e",
    "lint": "eslint . --ext .ts",
    "dev": "node dist/server/server.js",
    "curl:health": "curl -s http://localhost:3000/health",
    "curl:run": "curl -s -X POST http://localhost:3000/agent/run-once -H \"Content-Type: application/json\" -d @requests/run-once.json",
    "curl:run-goal": "curl -s -X POST http://localhost:3000/agent/run-once -H \"Content-Type: application/json\" -d @requests/run-once-goal.json",
    "curl:run-dry-with-llm": "curl -s -X POST http://localhost:3000/agent/run-once -H \"Content-Type: application/json\" -d @requests/run-once-dry-with-llm.json",
    "curl:run-dry-with-llm-goal": "curl -s -X POST http://localhost:3000/agent/run-once -H \"Content-Type: application/json\" -d @requests/run-once-dry-with-llm-goal.json",
    "curl:run-dry-without-llm": "curl -s -X POST http://localhost:3000/agent/run-once -H \"Content-Type: application/json\" -d @requests/run-once-dry-without-llm.json"
  },
  "dependencies": {
    "fastify": "^5.7.4",
    "pg": "^8.11.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "pg-mem": "^3.0.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5"
  }
}
