{
  "name": "cognitive-agent-wrapper",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "test:debug": "jest --runInBand --silent=false",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --runInBand --testPathPattern=tests/e2e",
    "lint": "eslint . --ext .ts",
    "dev": "node dist/server/server.js",
    "dev:mock": "LLM_PROVIDER=mock OBS_UI_ENABLED=true OBS_UI_SKIP_AUTH=true node dist/server/server.js",
    "request:run": "node scripts/run-request.cjs requests/run-once.json",
    "request:run-goal": "node scripts/run-request.cjs requests/run-once-goal.json",
    "request:run-dry-with-llm": "node scripts/run-request.cjs requests/run-once-dry-with-llm.json",
    "request:run-dry-with-llm-goal": "node scripts/run-request.cjs requests/run-once-dry-with-llm-goal.json",
    "request:run-dry-without-llm": "node scripts/run-request.cjs requests/run-once-dry-without-llm.json",
    "dev:test": "start-server-and-test dev:mock http://127.0.0.1:3000/health request:run-goal",
    "ernest-agent": "node dist/cli/agent-tui.js",
    "ui:dev": "npm run dev --prefix ui",
    "ui:build": "npm run build --prefix ui",
    "dev:ui": "npm run build && concurrently \"npm run dev\" \"npm run ui:dev\""
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "fastify": "^5.7.4",
    "pg": "^8.11.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "concurrently": "^9.2.1",
    "start-server-and-test": "^2.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "pg-mem": "^3.0.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5"
  }
}